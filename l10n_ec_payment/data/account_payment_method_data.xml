<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="account_payment_method_deposit_cheque_in" model="account.payment.method">
        <field name="name">Deposito Bancario Cheque</field>
        <field name="code">deposit_cheque</field>
        <field name="payment_type">inbound</field>
    </record>

    <record id="account_payment_method_credit_card_in" model="account.payment.method">
        <field name="name">Tarjeta de Crédito</field>
        <field name="code">card_credit</field>
        <field name="payment_type">inbound</field>
    </record>

    <record id="account_payment_method_transf_in" model="account.payment.method">
        <field name="name">Transferencia Bancaria</field>
        <field name="code">transf</field>
