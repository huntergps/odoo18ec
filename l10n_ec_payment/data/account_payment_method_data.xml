        <field name="name">Nota de Débito Bancaria</field>
        <field name="code">bank_debit</field>
        <field name="payment_type">outbound</field>
    </record>

    <record id="account_payment_method_transf_out" model="account.payment.method">
        <field name="name">Transferencia Bancaria</field>
        <field name="code">transf</field>
        <field name="payment_type">outbound</field>
    </record>



</odoo>
